<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.quanlythuchi">

    <application
        android:name=".base.App"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/ic_logo_wallet"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.QuanLyThuChi2"
        tools:targetApi="31">
        <activity android:name=".view.activity.splash.RoutingActivity"
            android:theme="@style/Theme.QuanLyThuChi2"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.authen.AuthenticationActivity"
            android:windowSoftInputMode="adjustResize"
            android:exported="true">

        </activity>
        <activity android:name=".view.activity.home.HomeActivity"
            android:windowSoftInputMode="adjustResize"
            android:exported="true"/>
    </application>

</manifest>